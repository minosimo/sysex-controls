<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="Ks37Book" parent="AdwBin">
    <property name="child">
      <object class="GtkStack">
        <property name="vhomogeneous">False</property>
        <property name="valign">start</property>

        <child>
          <object class="GtkStackPage">
            <property name="title" translatable="yes">Controller</property>
            <property name="child">
              <object class="Ks37PageController" />
            </property>
          </object>
        </child>

        <child>
          <object class="GtkStackPage">
            <property name="title" translatable="yes">Sequence</property>
            <property name="child">
              <object class="Ks37PageSequence" />
            </property>
          </object>
        </child>

        <child>
          <object class="GtkStackPage">
            <property name="title" translatable="yes">CV / Gate</property>
            <property name="child">
              <object class="Ks37PageCvGate" />
            </property>
          </object>
        </child>

        <child>
          <object class="GtkStackPage">
            <property name="title" translatable="yes">Transport</property>
            <property name="child">
              <object class="Ks37PageTransport" />
            </property>
          </object>
        </child>

        <child>
          <object class="GtkStackPage">
            <property name="title" translatable="yes">CC Bank 1</property>
            <property name="child">
              <object class="Ks37PageCcBank">
                <property name="midi-id-offset">0x2f</property>
              </object>
            </property>
          </object>
        </child>

        <child>
          <object class="GtkStackPage">
            <property name="title" translatable="yes">CC Bank 2</property>
            <property name="child">
              <object class="Ks37PageCcBank">
                <property name="midi-id-offset">0x3f</property>
              </object>
            </property>
          </object>
        </child>

        <child>
          <object class="GtkStackPage">
            <property name="title" translatable="yes">CC Bank 3</property>
            <property name="child">
              <object class="Ks37PageCcBank">
                <property name="midi-id-offset">0x4f</property>
              </object>
            </property>
          </object>
        </child>

        <child>
          <object class="GtkStackPage">
            <property name="title" translatable="yes">CC Bank 4</property>
            <property name="child">
              <object class="Ks37PageCcBank">
                <property name="midi-id-offset">0x5f</property>
              </object>
            </property>
          </object>
        </child>
      </object>
    </property>
  </template>
</interface>

